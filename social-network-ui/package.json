{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.css"
      ],
      "extensions": [
        "css"
      ]
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@mui/styles": "^5.12.3",
    "@mui/utils": "^5.12.3",
    "@mui/x-date-pickers": "^6.2.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "formik": "^2.2.9",
    
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.8.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "react-stomp": "^5.1.0",
    "redux-saga": "^1.2.3",
    "stompjs": "^2.3.3",
    "web-vitals": "^2.1.4",
    "yup": "^1.1.1"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-styles start-js",
    "build": "CI= npm run stylelint-build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint \"./**/*.js\" --cache --cache-strategy content --fix",
    "watch-styles": "npm-watch",
    "stylelint-fix": "stylelint --fix \"src/**/*.css\"",
    "stylelint-dev": "stylelint \"src/**/*.css\" --cache --cache-location \"./.stylelintcache\"; exit 0",
    "stylelint-build": "stylelint \"src/**/*.css\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3"
  }
}
